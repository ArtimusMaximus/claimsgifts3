    <link rel="stylesheet" href="/styles1.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <title>Sign up</title>
</head>
<body>
    <div class="jumbotron jumbotron-fluid">
        <div class="container">
            <h1 class="display-3">&nbsp;</h1>
        </div>
    </div>
    <!--section-->
        <h1 class="title"><strong>Sign up <svg xmlns="http://www.w3.org/2000/svg" width="48" height="48" fill="crimson" class="bi bi-check-lg" viewBox="0 0 16 16">
            <path d="M12.736 3.97a.733.733 0 0 1 1.047 0c.286.289.29.756.01 1.05L7.88 12.01a.733.733 0 0 1-1.065.02L3.217 8.384a.757.757 0 0 1 0-1.06.733.733 0 0 1 1.047 0l3.052 3.093 5.4-6.425a.247.247 0 0 1 .02-.022Z"/>
          </svg></strong></h1>
        <div class="container col-12-md">
            <div class="row">
                <div class="col-12-sm d-block">
                    <form action ="/signup" method="post">
                        <input class="form-control d-block" id="newusername" type="text" name="username" placeholder="Username" required="true">
                        <input class="form-control d-block" id="newuseremail" type="text" name="email" placeholder="Email" required="true">
                        <input class="form-control d-block" id="newuserpass" type="text" name="password" placeholder="Enter Password" required="true">              
                        <input class="form-control d-block" id="confirmnewuserpass" type="text" name="confirmpassword" placeholder="Confirm Password">              
                        <button class="btn-lg btn-outline-danger" style="margin-top: 10px; border-radius: 5px;" type="submit" value="register" id="signupbutton">Sign Up</button>
                    </form>
                    <div class="row present col-12-md" style="margin-top: 10px;"><h1><svg xmlns="http://www.w3.org/2000/svg" width="64" height="64" fill="crimson" class="bi bi-gift-fill" viewBox="0 0 16 16">
                        <path d="M3 2.5a2.5 2.5 0 0 1 5 0 2.5 2.5 0 0 1 5 0v.006c0 .07 0 .27-.038.494H15a1 1 0 0 1 1 1v1a1 1 0 0 1-1 1H1a1 1 0 0 1-1-1V4a1 1 0 0 1 1-1h2.038A2.968 2.968 0 0 1 3 2.506V2.5zm1.068.5H7v-.5a1.5 1.5 0 1 0-3 0c0 .085.002.274.045.43a.522.522 0 0 0 .023.07zM9 3h2.932a.56.56 0 0 0 .023-.07c.043-.156.045-.345.045-.43a1.5 1.5 0 0 0-3 0V3zm6 4v7.5a1.5 1.5 0 0 1-1.5 1.5H9V7h6zM2.5 16A1.5 1.5 0 0 1 1 14.5V7h6v9H2.5z"></path>
                        </svg></h1></div>
                </div>
            </div>
        </div>
    
    <div class="container sign col-12-md">
        <div class="row">
            <p class="font-italic">Return to&nbsp;&nbsp;<svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" fill="currentColor" class="bi bi-box-arrow-in-left" viewBox="0 0 16 16">
                <path fill-rule="evenodd" d="M10 3.5a.5.5 0 0 0-.5-.5h-8a.5.5 0 0 0-.5.5v9a.5.5 0 0 0 .5.5h8a.5.5 0 0 0 .5-.5v-2a.5.5 0 0 1 1 0v2A1.5 1.5 0 0 1 9.5 14h-8A1.5 1.5 0 0 1 0 12.5v-9A1.5 1.5 0 0 1 1.5 2h8A1.5 1.5 0 0 1 11 3.5v2a.5.5 0 0 1-1 0v-2z"/>
                <path fill-rule="evenodd" d="M4.146 8.354a.5.5 0 0 1 0-.708l3-3a.5.5 0 1 1 .708.708L5.707 7.5H14.5a.5.5 0 0 1 0 1H5.707l2.147 2.146a.5.5 0 0 1-.708.708l-3-3z"/>
              </svg>&nbsp;<a href="/login"><b>Log In</b></a></p>
        </div>
    </div>
    <div class="footer col-12-md"><a href="mailto:" style="color: black; text-decoration: none;">Report a</a> <svg xmlns="http://www.w3.org/2000/svg" width="32" height="32" fill="currentColor" class="bi bi-bug" viewBox="0 0 16 16">
